CREATE OR REPLACE VIEW public.vwfundscommitted AS 
 SELECT lender_no,
    lender_id,
    funding_committed,
    txn_date,
    loan_listing_number,
    funding_withdrawn,
    ( SELECT ll.loan_req_number
           FROM lms_m_loan_listing ll
          WHERE ll.loan_listing_number::text = f.loan_listing_number::text) AS loan_req_number,
    ( SELECT m.firstname
           FROM m_member m
          WHERE m.id = (( SELECT l.memmid
                   FROM m_lenders l
                  WHERE l.lender_id::text = f.lender_id::text))) AS lender_name
   FROM lms_t_funds_committed f;;
psql -h localhost -d postgres -U postgres 
pg_dump -h localhost -d p2plending_db -U postgres -t lms-Config > lms_config.sql
kubectl get pods -n p2p
kubectl top pods -n p2p
